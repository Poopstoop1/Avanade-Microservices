{

  "Routes": [
    {
      "DownstreamPathTemplate": "/api/estoque/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5285
        }
      ],
      "UpstreamPathTemplate": "/estoque/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "JwtBearer",
        "AllowedScopes": []
      }
    },
    {
      "DownstreamPathTemplate": "/api/vendas/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5156
        }
      ],
      "UpstreamPathTemplate": "/vendas/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "JwtBearer",
        "AllowedScopes": []
      }
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "estoque",
      "Config": [
        {
          "Name": "Estoque API",
          "Version": "v1",
          "Url": "http://localhost:5285/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "vendas",
      "Config": [
        {
          "Name": "Vendas API",
          "Version": "v1",
          "Url": "http://localhost:5156/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}
